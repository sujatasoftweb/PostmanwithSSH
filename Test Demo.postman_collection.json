					"name": "List Users",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"let baseurl = pm.environment.get(\"Baseurl\");\r",
									"console.log(\"base url variable is :\" + baseurl);\r",
									"\r",
									"bodyData= JSON.parse(responseBody);\r",
									"value= bodyData.data[1].first_name;\r",
									"console.log(value);\r",
									"pm.environment.set(\"first_name\", value);\r",
									"tests[\"Verify the response code\"] = responseCode.code == 200;\r",
									"\r",
									" tests [\"Verify the responsebody\"] = responseBody.has(\"michael.lawson@reqres.in\");\r",
									"\r",
									"     var response = JSON.parse(responseBody);\r",
									"     tests[\"verify the page no\"] = response.page == 2;"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{Baseurl}}/api/users?page=2",
